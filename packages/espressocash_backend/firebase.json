{
  "hosting": [
    {
      "target": "main",
      "appAssociation": "AUTO",
      "rewrites": [
        {
          "source": "/**",
          "dynamicLinks": true
        }
      ]
    },
    {
      "target": "link",
      "appAssociation": "NONE",
      "rewrites": [
        {
          "source": "**",
          "run": {
            "serviceId": "backend",
            "region": "europe-west1"
          }
        }
      ]
    }
  ]
}
